---
import TicketLogic from "../components/ticket/TicketLogic";
import Layout from "../layout/Layout.astro";
---

<Layout title="ตั๋วของคุณ — Intania Open House 2023">
  <main class="">
    <TicketLogic client:load />
  </main>
</Layout>

<script>
  const name = localStorage.getItem("name");
  const major = localStorage.getItem("major");
  if (!name || !major) {
    window.location.href = "/game";
  }
</script>
